		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"preset": "jest-expo",
		"testMatch": [
			"**/__tests__/**/*.[tj]s?(x)",
			"**/?(*.)(spec|test).[tj]s?(x)"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		}
	},
	"dependencies": {
		"@expo/react-native-action-sheet": "^1.0.2",
		"buffer": "^5.2.1",
		"expo": "^29.0.0",
		"native-base": "^2.8.0",
		"react": "16.3.1",
		"react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
		"react-native-svg": "^6.5.2",
		"react-native-svg-uri": "^1.2.3",
		"react-navigation": "^2.9.3"
	}
